{
  "name": "bgis",
  "version": "1.0.0",
  "description": "bgis mapping client",
  "keywords": [
    "map",
    "mapping",
    "bgis",
    "openlayers"
  ],
  "private": true,
  "homepage": "https://bgis-app.github.io/",
  "main": "./bgis.bundle.js",
  "types": "./types/src/index.d.ts",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/bgis-app/bgis-core.git"
  },
  "bugs": {
    "url": "https://github.com/bgis-app/bgis-core/issues"
  },
  "scripts": {
    "start": "yarn run cleanup-dev && set DEBUG=express:* && webpack-dev-server --config webpack/webpack.dev.js --open --watch --port 8081",
    "build-package": "yarn run cleanup-build && webpack --config webpack/webpack.prod.js && node tasks/prepare-package.js",
    "build-site": "rimraf build/site && webpack --config webpack/webpack.site.js && yarn run apidoc && node tasks/prepare-site.js",
    "build:watch": "yarn build --watch --progress",
    "apidoc": "typedoc --tsconfig tsconfig.json",
    "lint:eslint": "eslint . --ext .js,.ts",
    "lint:stylelint": "stylelint **/*.scss",
    "lint": "yarn run lint:eslint && yarn run lint:stylelint",
    "lint:fix": "yarn run lint --fix",
    "test": "jest --coverage --logHeapUsage -w=2 --config test/jest.config.js",
    "test:watch": "yarn run test --watch",
    "cleanup-build": "rimraf build/bgis/",
    "cleanup-dev": "rimraf build/dev/"
  },
  "peerDependencies": {
    "ol": ">=6.6.1 <7",
    "proj4": ">=2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@types/geojson": "^7946.0.7",
    "@types/jest": "^26.0.20",
    "@types/proj4": "^2.5.0",
    "@typescript-eslint/eslint-plugin": "^4.26.1",
    "@typescript-eslint/eslint-plugin-tslint": "^4.26.1",
    "@typescript-eslint/parser": "^4.26.1",
    "autoprefixer": "^10.2.3",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.1.0",
    "canvas": "^2.8.0",
    "clean-webpack-plugin": "^3.0.0",
    "codelyzer": "^6.0.1",
    "core-js": "^3.6.5",
    "css-loader": "^5.0.0",
    "eslint": "^7.28.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.1.0",
    "jest": "^26.6.3",
    "jest-canvas-mock": "^2.3.1",
    "mini-css-extract-plugin": "^1.2.1",
    "ol": "^6.6.1",
    "optimize-css-assets-webpack-plugin": "^5.0.4",
    "postcss": "^8.2.4",
    "postcss-loader": "^4.1.0",
    "proj4": "^2.7.0",
    "pug": "^3.0.0",
    "pug-loader": "^2.4.0",
    "regenerator-runtime": "^0.13.7",
    "sass": "^1.34.1",
    "sass-loader": "^10.0.5",
    "style-loader": "^1.3.0",
    "stylelint": "^13.8.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-scss": "^3.18.0",
    "terser-webpack-plugin": "^5.1.1",
    "ts-loader": "^8.0.8",
    "tslint": "^6.1.3",
    "typedoc": "^0.21.4",
    "typescript": "^4.3.5",
    "url-loader": "^4.1.1",
    "webpack": "^5.18.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
