{
  "name": "bgis",
  "version": "1.1.0",
  "description": "bgis mapping client",
  "keywords": [
    "map",
    "mapping",
    "bgis",
    "openlayers"
  ],
  "private": true,
  "homepage": "https://bgis-app.github.io/",
  "main": "./bgis.bundle.js",
  "types": "./types/index.d.ts",
  "license": "BSD-2-Clause",
  "repository": {
    "type": "git",
    "url": "git://github.com/bgis-app/bgis-core.git"
  },
  "bugs": {
    "url": "https://github.com/bgis-app/bgis-core/issues"
  },
  "scripts": {
    "start": "yarn run cleanup-dev && webpack-dev-server --config webpack/webpack.dev.js --open --port 8081",
    "build-package": "yarn run cleanup-build && webpack --config webpack/webpack.prod.js && node tasks/prepare-package.js",
    "build-site": "rimraf build/site && webpack --config webpack/webpack.site.js && yarn run apidoc && node tasks/prepare-site.js",
    "apidoc": "typedoc --tsconfig tsconfig.json",
    "lint:eslint": "eslint . --ext .ts",
    "lint:stylelint": "stylelint **/*.scss",
    "lint": "yarn run lint:eslint && yarn run lint:stylelint",
    "lint:fix": "yarn run lint --fix",
    "test": "jest --coverage --logHeapUsage -w=2 --config test/jest.config.js",
    "test:watch": "yarn run test --watch",
    "cleanup-build": "rimraf build/bgis/",
    "cleanup-dev": "rimraf build/dev/"
  },
  "peerDependencies": {
    "ol": ">=7.1 <8",
    "proj4": ">=2.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@babel/preset-env": "^7.19.3",
    "@babel/preset-typescript": "^7.18.6",
    "@types/geojson": "^7946.0.8",
    "@types/jest": "^29.1.1",
    "@types/proj4": "^2.5.2",
    "@typescript-eslint/eslint-plugin": "^5.39.0",
    "@typescript-eslint/parser": "^5.39.0",
    "autoprefixer": "^10.4.13",
    "babel-jest": "^29.1.2",
    "babel-loader": "^8.2.5",
    "canvas": "^2.11.2sign",
    "clean-webpack-plugin": "^4.0.0",
    "core-js": "^3.22.2",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^4.2.0",
    "eslint": "^8.24.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^29.1.2",
    "jest-canvas-mock": "^2.3.1",
    "jest-environment-jsdom": "^28.0.1",
    "mini-css-extract-plugin": "^2.6.0",
    "ol": "^7.1.0",
    "postcss": "^8.4.21",
    "postcss-import": "^14.1.0",
    "postcss-loader": "^6.2.1",
    "postcss-replace": "^2.0.1",
    "proj4": "^2.8.0",
    "pug": "^2.0.4",
    "pug-loader": "^2.4.0",
    "regenerator-runtime": "^0.13.9",
    "sass": "^1.58.3",
    "sass-loader": "^12.6.0",
    "source-sans": "^3.46.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.13.0",
    "stylelint-config-recommended-scss": "^7.0.0",
    "stylelint-scss": "^4.3.0",
    "tailwindcss": "^3.2.7",
    "ts-loader": "^9.2.9",
    "typedoc": "^0.23.15",
    "typescript": "^4.8.4",
    "webpack": "^5.88.1",
    "webpack-bundle-analyzer": "^4.6.1",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.11.1"
  }
}
